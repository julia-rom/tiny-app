<h1>Tiny App Nav</h1>

<% if (username) { %>
<form method="POST" action="/logout">
    <%= `Hello ${username}`%>
    <button type="logout">Log Out</button>
</form>
<% } else { %>
<form method="POST" action="/login">
    <input type="text" name="username" value="Enter username">
    <button type="submit">Submit</button>
</form>
<% } %>